{
  "name": "prototyper",
  "version": "0.1.1",
  "description": "Monterey mini-version, used for rapid prototypingof GUI comnponents",
  "keywords": [
    "aurelia",
    "monterey",
    "prototyping",
    "typescript",
    "gui"
  ],
  "homepage": "",
  "bugs": {
    "url": "https://github.com/monterey-framework/prototyper/issues"
  },
  "main": "app/index.js",
  "license": "CC0-1.0",
  "author": {
    "name": "aureliatools",
    "email": "info@aureliatools.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monterey-framework/prototyper"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "gulp clean-installer-dist && gulp build && build",
    "setup": "node utils -setup && node utils -rebuild-native",
    "rebuild-native": "node utils -rebuild-native"
  },
  "directories": {
    "app": "app"
  },
  "build": {
    "appId": "monterey",
    "app-category-type": "monterey",
    "win": {
      "iconUrl": "https://i.imgur.com/bC47YNC.png"
    },
    "mac": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },  
  "devDependencies": {
    "asar": "^0.11.0",
    "aurelia-bundler": "^0.4.0",
    "aurelia-tools": "^0.2.3",
    "changelog": "^1.0.7",
    "conventional-changelog": "1.1.0",
    "del": "^2.2.1",
    "electron-builder": "5.26.0",
    "electron-connect": "^0.4.8",
    "electron-prebuilt": "1.4.1",
    "electron-rebuild": "^1.2.0",
    "es6-module-loader": "^0.17.11",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.4.0",
    "gulp-changed": "^1.3.0",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-less": "^3.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-protractor": "2.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^2.13.6",
    "gulp-watch": "^4.3.9",
    "gulp-zip": "^3.2.0",
    "isparta": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jasmine-core": "^2.4.1",
    "jspm": "^0.16.15",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jspm": "2.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-systemjs": "^0.14.0",
    "mock-require": "^1.3.0",
    "object.assign": "^4.0.3",
    "open": "0.0.5",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.1",
    "systemjs": "^0.19.31",
    "tslint": "^3.13.0",
    "typescript": "^2.0.0",
    "vinyl-paths": "^2.1.0",
    "yargs": "^6.3.0"
  }
}
